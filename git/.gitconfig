[core]
	editor = vim
	whitespace = trailing-space,space-before-tab
[alias]
    co = checkout
[color]
	ui = true
[push]
	default = current
[mergetool]
    keepBackup = false
    keepTemporaries = false
    prompt = false
[color "status"]
    added = green
    changed = yellow
    untracked = red
